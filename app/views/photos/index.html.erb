<h1>Photos#index</h1>
<p>Find me in app/views/photos/index.html.erb</p>

<% @photos.each do |image| %>
<%= link_to photo_path(image.id) do %>
    <li>
      <%= image.title %>
      <%= image_tag image.avatar %>
      <% image.photo_tags.each do |t| %>
	      <%= t.tag.tag_name %>
	      <% end %>
    </li>
    <% end %>
  <% end %>

